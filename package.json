{
  "name": "g5-msr",
  "version": "0.3.10",
  "description": "See what your service dollar gets you!",
  "author": "David Miller & Tyler Hasenoehrl",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "docker:build": "docker build -t msr . --build-arg NPM_TOKEN=$NPM_TOKEN",
    "docker:run": "docker run -p 8080:8080 -t msr",
    "helm:upgrade": "helm upgrade msr ./chart/nuxt",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "dev-debug": "node --inspect server/index.js"
  },
  "dependencies": {
    "@cloudnative/health-connect": "^2.1.0",
    "@getg5/g5-auth": "^2.0.9",
    "@getg5/g5-updatable": "^2.2.3",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "apexcharts": "^3.20.0",
    "appmetrics-prometheus": "^3.1.0",
    "appmetrics-zipkin": "^1.1.1",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.16.0",
    "bull": "^3.18.0",
    "bull-arena": "^2.8.3",
    "cross-env": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "greenlock": "^4.0.4",
    "hashids": "^2.2.1",
    "jquery": "^3.5.1",
    "jsforce": "^1.9.3",
    "nuxt": "^2.14.4",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "sequelize": "^5.22.3",
    "tiptap": "^1.29.6",
    "tiptap-extensions": "^1.32.5",
    "vue-apexcharts": "^1.6.0",
    "vue-multiselect": "^2.1.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@vue/test-utils": "^1.0.4",
    "axios-mock-adapter": "^1.18.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.3.0",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "jest": "^26.4.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "sass-loader": "^8.0.2",
    "vue-jest": "^3.0.6"
  }
}
